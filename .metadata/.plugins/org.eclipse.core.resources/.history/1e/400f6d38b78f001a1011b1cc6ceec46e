package Queue;

public class Queue {
	private int front;
	private int size;
	private int[] data;
	
	//parameterized constructor
	public Queue(int cap) {
		this.front = 0; //initially the front of the queue is 0
		this.size = 0; //initially size is 0
		this.data = new int[cap]; //creating a new array data with cap as capacity
	}

	//default constructor
	public Queue() { 
		this(5); //
	}

	//
	public void enqueue(int item) {
		int ni = (this.front + this.size) % this.data.length;
		this.data[ni] = item;
		this.size++;
	}
	
	public 
}
