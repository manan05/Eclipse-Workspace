package Queue;

public class Queue {
	private int front;
	private int size;
	private int[] data;
	
	public Queue(int cap) {
		this.front = 0;
		this.size = 0;
		this.data = new int[cap];
	}
	
	public Queue() {
		this(5);
	}
	
	public void enqueue(int item) {
		int ni = (this.front + this.size)%this.data.length;
		this.data[ni] = item;
		this.size ++;
	}
}

